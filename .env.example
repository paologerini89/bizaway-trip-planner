# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# API Configuration
TRIPS_API_ENDPOINT=https://your-api-endpoint-here
TRIPS_API_KEY=your-api-key-here

# Cache Configuration
CACHE_TYPE=memory
CACHE_TTL_SECONDS=300

# Rate Limiting Configuration
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=1 minute

# Redis Configuration (opzionale - solo se CACHE_TYPE=redis)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
